{
  "modules": [
    {
      "description": "Gets title from URL in text",
      "expression": "http:|https:",
      "command": "URL-detection",
      "module": "get_title"
    },
    {
      "description": "Usage: !ilm <location>\nFetches weather info for location, default Tallinn",
      "expression": "^!ilm$|^!ilm .*$",
      "command": "!ilm",
      "module": "get_weather"
    },
    {
      "description": "Usage: !kmh_to_ms 10",
      "expression": "^!kmh_to_ms .*$",
      "command": "!kmh_to_ms",
      "module": "kmh_to_ms"
    },
    {
      "description": "Usage: !pic fetches random image url from pic_reddit_urls.txt and makes request to given subreddit and chooses random image",
      "expression": "^!pic",
      "command": "!pic",
      "module": "get_pic"
    },
    {
      "description": "Usage: !update\nPulls latest changes from github",
      "expression": "^!update$",
      "command": "!update",
      "module": "get_updates"
    },
    {
      "description": "Usage: !sysinfo\nShows uptime and memory usage",
      "expression": "^!sysinfo$",
      "command": "!sysinfo",
      "module": "get_sysinfo"
    },
    {
      "description": "Usage: !head <url>\nGets HEAD",
      "expression": "^!head http",
      "command": "!head",
      "module": "get_head"
    },
    {
      "description": "Usage: !remindme someone does what",
      "expression": "^!remindme .*$",
      "command": "!remindme",
      "module": "remindme"
    },
    {
      "description": "Usage: !reminders <who>",
      "expression": "^!reminders",
      "command": "!reminders",
      "module": "reminders"
    },
    {
      "description": "Usage: !put_useless a thing that is subjectivly useless",
      "expression": "^!put_useless .*$",
      "command": "!put_useless",
      "module": "put_useless"
    },
    {
      "description": "Usage: !get_useless <who>",
      "expression": "^!get_useless.*",
      "command": "!get_useless",
      "module": "get_useless"
    },
    {
      "description": "Usage: !del_useless <id|content>",
      "expression": "^!del_useless.*",
      "command": "!del_useless",
      "module": "del_useless"
    },
    {
      "description": "Usage: !chuck",
      "expression": "^!chuck",
      "command": "!chuck",
      "module": "chuck"
    },
    {
      "description": "Usage: !insult",
      "expression": "^!insult.*|^!t√§na",
      "command": "!insult",
      "module": "insult"
    },
    {
      "description": "Usage: !corona fetches latest statistics about the spread of corona virus",
      "expression": "^!corona$",
      "command": "!corona",
      "module": "get_corona"
    }
  ]
}
